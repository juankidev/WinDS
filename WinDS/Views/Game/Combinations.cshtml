@{
    List<string> combinations = ViewBag.Combinations;
}

<table class="table table-hover" id="partial-table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Word</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in combinations)
        {
            <tr>
                <th scope="row">@( combinations.IndexOf(item) + 1)</th>
                <td>@item</td>
            </tr>
        }

    </tbody>
</table>

<script type="text/javascript">
    $( document ).ready(function() {
        $("#partial-table").DataTable();
    });
</script>
